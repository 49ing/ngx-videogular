{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": "./",
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@videogular/npx-videogular": ["projects/videogular/npx-videogular/src/public-api"],
      "@videogular/npx-videogular/*": ["projects/videogular/npx-videogular/src/lib/*"],
      "@angular/*": ["node_modules/@angular/*"],
      "tslib": ["node_modules/tslib"]
    },
    "allowSyntheticDefaultImports": true
  }
}
