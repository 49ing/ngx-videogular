{
  "name": "ngx-videogular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && ng build --prod",
    "build:lib": "ng build @videogular/ngx-videogular --prod && npm run copy-files",
    "copy-files": "cp README.md dist/videogular/ngx-videogular/README.md && cp LICENSE dist/videogular/ngx-videogular/LICENSE",
    "package": "npm run build:lib",
    "prepack": "npm run package",
    "pack": "npm pack ./dist/videogular/ngx-videogular",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.5",
    "@angular/common": "~9.1.5",
    "@angular/compiler": "~9.1.5",
    "@angular/core": "~9.1.5",
    "@angular/forms": "~9.1.5",
    "@angular/platform-browser": "~9.1.5",
    "@angular/platform-browser-dynamic": "~9.1.5",
    "@angular/router": "~9.1.5",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular-devkit/build-ng-packagr": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "~9.1.5",
    "@angular/language-service": "~9.1.5",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.13.5",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^5.0.5",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^9.1.3",
    "protractor": "~5.4.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.1",
    "typescript": "~3.8.3"
  }
}
